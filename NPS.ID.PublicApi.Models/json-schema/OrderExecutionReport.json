{
  "title": "OrderExecutionReport",
  "description": "In response to an order creation or modification request described in Creating and modifying orders and for any order updates, the Intraday Platform will send an Order Execution Report message.",
  "definitions": {
    "Error": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "errorCode": {
          "type": "string",
          "enum": [
            "MISSING_REQUIRED_FIELD",
            "ILLEGAL_FIELD",
            "FIELD_OUT_OF_RANGE",
            "FIELD_FORMAT_INVALID",
            "ITEM_NOT_FOUND",
            "ACCESS_MODE_VIOLATION",
            "THIRD_PARTY"
          ]
        },
        "errorMessage": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "required": [
        "errorCode",
        "errorMessage"
      ]
    },
    "OrderExecutionEntry": {
      "type": [
        "object",
        "null"
      ],
      "properties": {
        "eventSequenceNo": {
          "type": "integer"
        },
        "marketId": {
          "type": [
            "string",
            "null"
          ]
        },
        "tenantId": {
          "type": [
            "string",
            "null"
          ]
        },
        "userId": {
          "type": [
            "string",
            "null"
          ]
        },
        "orderId": {
          "type": [
            "string",
            "null"
          ]
        },
        "revisionNo": {
          "type": "integer"
        },
        "previousOrderId": {
          "description": "Id of the previous order in this modification chain. When an order is modified and its priority changes, or partially matched, a new order Id is assigned.",
          "type": [
            "string",
            "null"
          ]
        },
        "originalOrderId": {
          "description": "The original order id in this modification chain.",
          "type": [
            "string",
            "null"
          ]
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "clientOrderId": {
          "description": "UUID for the order, provided by the client to track their own orders",
          "type": [
            "string",
            "null"
          ]
        },
        "portfolioId": {
          "description": "The portfolio id of the current order",
          "type": [
            "string",
            "null"
          ]
        },
        "contractIds": {
          "description": "The contract ids for current order",
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": [
              "string",
              "null"
            ]
          }
        },
        "deliveryAreaId": {
          "description": "The delivery area id of the current order.",
          "type": "integer"
        },
        "side": {
          "description": "BUY/SELL",
          "type": "string",
          "enum": [
            "BUY",
            "SELL"
          ]
        },
        "orderType": {
          "description": "LIMIT, ICEBERG, USER_DEFINED_BLOCK",
          "type": "string",
          "enum": [
            "LIMIT",
            "ICEBERG",
            "USER_DEFINED_BLOCK"
          ]
        },
        "unitPrice": {
          "description": "Price in Euro cents or pence (based on currency for the area)",
          "type": "integer"
        },
        "quantity": {
          "description": "Quantity in kW",
          "type": "integer"
        },
        "timeInForce": {
          "description": "IOC, FOK, AON, NON, GTD, GFS",
          "type": "string",
          "enum": [
            "IOC",
            "FOK",
            "NON",
            "GTD",
            "GFS"
          ]
        },
        "expireTime": {
          "description": "If timeInForce is set to GTD (Good Till Date), the expireTime will determine when the order expires",
          "type": "string",
          "format": "date-time"
        },
        "text": {
          "type": [
            "string",
            "null"
          ]
        },
        "state": {
          "description": "ACTI — Active, IACT — Closed, matched(will never be reopened), HIBE — Deactivated(can be reopened)",
          "type": "string",
          "enum": [
            "PENDING",
            "ACTI",
            "HIBE",
            "IACT",
            "REJECTED"
          ]
        },
        "action": {
          "type": "string",
          "enum": [
            "USER_ADDED",
            "USER_HIBERNATED",
            "USER_MODIFIED",
            "USER_DELETED",
            "SYSTEM_HIBERNATED",
            "SYSTEM_MODIFIED",
            "SYSTEM_DELETED",
            "PARTIAL_EXECUTION",
            "FULL_EXECUTION",
            "ICEBERG_SLICE_ADDED"
          ]
        },
        "clipSize": {
          "description": "For iceberg orders only; the size of one clip",
          "type": "integer"
        },
        "clipPriceChange": {
          "description": "For iceberg orders only; the price change after each clip is consumed",
          "type": "integer"
        },
        "remainingQuantity": {
          "type": "integer"
        },
        "errors": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "$ref": "#/definitions/Error"
          }
        }
      },
      "required": [
        "eventSequenceNo",
        "marketId",
        "tenantId",
        "userId",
        "orderId",
        "revisionNo",
        "previousOrderId",
        "originalOrderId",
        "updatedAt",
        "clientOrderId",
        "portfolioId",
        "contractIds",
        "deliveryAreaId",
        "side",
        "orderType",
        "unitPrice",
        "quantity",
        "timeInForce",
        "expireTime",
        "text",
        "state",
        "action",
        "clipSize",
        "clipPriceChange",
        "remainingQuantity",
        "errors"
      ]
    }
  },
  "type": "object",
  "properties": {
    "requestId": {
      "description": "Unique identifier for this request, provided by the client to track their own requests",
      "type": [
        "string",
        "null"
      ]
    },
    "memberId": {
      "description": "Member ID",
      "type": [
        "string",
        "null"
      ]
    },
    "errorType": {
      "description": "Technical field about message originator.",
      "type": "string",
      "enum": [
        "NPM",
        "PRE_TRADE",
        "MW",
        "LTS",
        "XBID"
      ]
    },
    "errors": {
      "description": "An array of objects that contain fields errorCode and errorMessage.",
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/Error"
      }
    },
    "orders": {
      "description": "Order execution entries",
      "type": [
        "array",
        "null"
      ],
      "items": {
        "$ref": "#/definitions/OrderExecutionEntry"
      }
    },
    "updatedAt": {
      "description": "Last modification time (status change) of data",
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "requestId",
    "memberId",
    "errorType",
    "errors",
    "orders",
    "updatedAt"
  ]
}